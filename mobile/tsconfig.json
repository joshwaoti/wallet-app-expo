{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "noEmit": true
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "app/_layout.jsx",
    "app/index.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "assets/styles/**/*.js"
  ],
  "extends": "expo/tsconfig.base"
}
